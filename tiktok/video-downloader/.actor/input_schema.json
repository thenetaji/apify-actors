{
  "schemaVersion": 1,
  "title": "TikTok Video downloader input fields",
  "description": "Input TikTok Video URL and configure other options",
  "type": "object",
  "properties": {
    "urls": {
      "title": "Video URLs",
      "type": "array",
      "description": "The URL of the video to download",
      "editor": "requestListSources",
      "prefill": [
        { "url": "https://vt.tiktok.com/ZSMy827Qe/" }
      ],
      "default": [
        { "url": "https://vt.tiktok.com/ZSMy827Qe/" }
      ]
    },
    "quality": {
      "title": "Select quality",
      "description": "Select video quality to download",
      "type": "string",
      "editor": "select",
      "default": "best",
      "prefill": "best",
      "enum": [
        "best",
        "worst"
      ],
      "enumTitles": [
        "best (recommended)",
        "worst"
      ]
    },
    "format": {
      "title": "Select file type (can be slow)",
      "description": "Select video file type (.mp4, .mkv, .webm)",
      "type": "string",
      "editor": "select",
      "default": "default",
      "prefill": "default",
      "enum": ["default", "mp4", "webm", "mkv"],
      "enumTitles": ["default", "mp4", "webm", "mkv"]
    },
    "proxy": {
      "title": "Use proxy",
      "type": "object",
      "editor": "proxy",
      "description": "Enable proxy for downloading, only residential proxy would be used as others don't work",
      "default": {
        "useApifyProxy": true,
        "apifyProxyGroups": ["RESIDENTIAL"]
      }
    }
  },
  "required": ["urls", "proxy", "format", "quality"]
}
